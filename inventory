[consul_instances]
raspberrypi-1.home consul_node_role=server ansible_ssh_user=pi ansible_ssh_private_key_file=./id_rsa

[consul_instances:vars]
consul_version=1.12.3
consul_architecture=arm 
consul_client_address=192.168.1.40
consul_connect_enabled=true
consul_encrypt_enable=true
consul_raw_key={{ lookup('env','GOSSIP_KEY') }}
consul_raft_protocol=3
consul_bootstrap_expect=true
consul_bootstrap_expect_value=1

[vault_instances]
raspberrypi-1.home ansible_ssh_user=pi ansible_ssh_private_key_file=./id_rsa

[vault_instances:vars]
vault_backend=consul
vault_consul=192.168.1.40:8500
